{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"forceConsistentCasingInFileNames": true,
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmitOnError": true,
		"outDir": "./dist",
		"pretty": true,
		"rootDir": "./src",
		"sourceMap": true,
    "strict": true,
		"target": "esnext"
	}
}
